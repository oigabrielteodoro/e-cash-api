{
  "name": "api",
  "version": "0.0.1",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --respawn --ignore-watch node_modules src/index.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:ci": "jest --passWithNoTests --ci --watchAll=false",
    "prettier:write": "prettier --write 'src/**/**.{js,ts,json,md}'",
    "prettier:check": "prettier -c  'src/**/**.{js,ts,json,md}'",
    "lint": "eslint 'src/**/**.{js,ts}'",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "big.js": "6.1.1",
    "celebrate": "15.0.0",
    "class-transformer": "0.5.1",
    "cors": "2.8.5",
    "express": "4.17.2",
    "express-async-errors": "3.1.1",
    "jsonwebtoken": "8.5.1",
    "pg": "8.7.1",
    "reflect-metadata": "0.1.13",
    "ts-node-dev": "1.1.8",
    "tsconfig-paths": "3.12.0",
    "tsyringe": "4.6.0",
    "typeorm": "0.2.41"
  },
  "devDependencies": {
    "@babel/cli": "7.10.4",
    "@babel/core": "7.10.4",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-decorators": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@types/bcryptjs": "2.4.2",
    "@types/big.js": "6.1.2",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.0",
    "@types/jsonwebtoken": "8.5.6",
    "@types/module-alias": "2.0.1",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "babel-plugin-root-import": "6.6.0",
    "babel-plugin-transform-decorators": "6.24.1",
    "babel-plugin-transform-typescript-metadata": "0.3.0",
    "dotenv": "10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "fishery": "2.1.0",
    "husky": "4",
    "jest": "27.4.7",
    "lint-staged": "12.1.7",
    "module-alias": "2.2.2",
    "prettier": "2.5.1",
    "ts-jest": "27.1.2",
    "typescript": "4.5.4",
    "uuid": "8.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/**.{js,jsx,ts,tsx}": [
      "eslint"
    ],
    "src/**/**.{js,jsx,ts,tsx,json,css,less,md}": [
      "prettier --write"
    ]
  }
}
